{
    "name": "meldcx-file-sharing-api",
    "version": "1.0.0",
    "description": "Backend Code Test of MeldCX - Build a File Sharing API Server ",
    "main": "build/src/index.js",
    "scripts": {
        "start:watch": "nodemon --watch \"build/**/*\" -e js --exec \"node -r source-map-support/register -r @swc-node/register ./build/src/index.js\"",
        "start:dev": "concurrently \"npm run compile:watch\" \"npm run start:watch\"",
        "start:prod": "node -r source-map-support/register -r @swc-node/register ./build/src/index.js",
        "compile": "swc src -d build --source-maps=inline",
        "compile:watch": "swc src -d build --source-maps=inline --watch",
        "build": "rimraf ./build && swc ./src -d build --source-maps",
        "test": "npx jest",
        "prettier": "prettier --write ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.540.0",
        "@swc-node/register": "^1.9.0",
        "@swc/cli": "^0.3.10",
        "@swc/core": "^1.4.8",
        "async": "^3.2.5",
        "axios": "^1.6.8",
        "body-parser": "^1.20.2",
        "concurrently": "^8.2.2",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "date-fns-tz": "^2.0.1",
        "dotenv": "^16.4.5",
        "express": "^4.18.3",
        "express-rate-limit": "^7.2.0",
        "ioredis": "^5.3.2",
        "moment": "^2.30.1",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.9.2",
        "node-cron": "^3.0.3",
        "nodemon": "^3.1.0",
        "reflect-metadata": "^0.2.1",
        "rimraf": "^5.0.5",
        "sequelize": "^6.37.1",
        "source-map-support": "^0.5.21",
        "uuid": "^9.0.1",
        "validatorjs": "^3.22.1",
        "winston": "^3.12.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.30",
        "@types/sequelize": "^4.28.20",
        "@types/uuid": "^9.0.8",
        "@types/validatorjs": "^3.15.5",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.3"
    }
}
